{
  "source": {
    "include": ["./src/", "./README.md"],
    "includePattern": "\\.(js|ts)$",
    "exclude": ["node_modules/", "tests/", "coverage/"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}